{
  "name": "parcel-vuejs",
  "description": "Simple Vue.js app",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Jason Kumpf",
    "email": "kumpf.jason@gmail.com",
    "url": "https://jasonkumpf.com/"
  },
  "license": "MIT",
  "scripts": {
    "prestart": "rm -rf .cache",
    "prebuild": "rm -rf .cache && rm -rf build",
    "predist": "rm -rf .cache && rm -rf dist",
    "start": "parcel src/index.html --no-minify --public-url /",
    "build": "parcel build src/index.html --out-dir build --no-minify --public-url /",
    "dist": "parcel build src/index.html --out-dir dist --public-url /"
  },
  "dependencies": {
    "vue": "^2.5.10",
    "vue-hot-reload-api": "^2.3.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.3.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-vue": "^2.0.0",
    "parcel-bundler": "^1.9.6",
    "vue-template-compiler": "^2.5.16"
  }
}
